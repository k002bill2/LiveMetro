{
  "name": "livemetro-app",
  "version": "1.0.0",
  "description": "실시간 전철 알림 - Real-time subway notification app for Seoul metropolitan area",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "expo build",
    "build:development": "eas build --platform all --profile development",
    "build:preview": "eas build --platform all --profile preview",
    "build:production": "eas build --platform all --profile production",
    "build:ios": "eas build --platform ios --profile production-ios",
    "build:android": "eas build --platform android --profile production-android",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "submit:all": "eas submit --platform all",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prebuild": "npm run lint && npm run type-check",
    "presubmit": "npm run lint && npm run type-check && npm run test",
    "skills:update": "ts-node scripts/skillsGuideUpdater.ts",
    "skills:update:force": "ts-node scripts/skillsGuideUpdater.ts --force",
    "skills:check": "ts-node scripts/skillsGuideUpdater.ts --check-only",
    "backup:claude": "ts-node scripts/backupClaudeConfig.ts",
    "restore:claude": "ts-node scripts/restoreClaudeConfig.ts",
    "restore:claude:latest": "ts-node scripts/restoreClaudeConfig.ts --latest",
    "restore:claude:list": "ts-node scripts/restoreClaudeConfig.ts --list",
    "clone:claude": "ts-node scripts/cloneClaudeConfig.ts",
    "clone:claude:check": "ts-node scripts/cloneClaudeConfig.ts --dry-run"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.25",
    "@react-navigation/stack": "^6.3.20",
    "expo": "~49.0.15",
    "expo-linear-gradient": "~12.3.0",
    "expo-location": "~16.1.0",
    "expo-notifications": "~0.20.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "firebase": "^10.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-web": "~0.19.6",
    "react-native-webview": "^13.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.5",
    "@types/node": "^25.0.3",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.50.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.3"
  },
  "keywords": [
    "subway",
    "notification",
    "real-time",
    "react-native",
    "firebase",
    "seoul",
    "transportation"
  ],
  "author": "LiveMetro Team",
  "license": "MIT",
  "private": true
}
