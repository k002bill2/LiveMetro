# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id

# External APIs
SEOUL_SUBWAY_API_KEY=your_seoul_subway_api_key
SEOUL_SUBWAY_API_BASE_URL=http://swopenapi.seoul.go.kr/api/subway

# AWS S3 Configuration (for asset storage)
AWS_S3_BUCKET=your_s3_bucket_name
AWS_S3_REGION=ap-northeast-2
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# App Configuration
APP_ENV=development
API_TIMEOUT=10000
NOTIFICATION_DELAY_THRESHOLD=5
CACHE_DURATION_MINUTES=30

# Analytics (optional)
ANALYTICS_TRACKING_ID=your_analytics_id

# Feature Flags
ENABLE_CONGESTION_DATA=true
ENABLE_ML_PREDICTIONS=false
ENABLE_PREMIUM_FEATURES=true
ENABLE_DEBUG_LOGGING=true